<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
		<Dialog id="idDialog1" title="{i18n>ZLBL_Title1}">
		<f:SimpleForm id="idSimpleForm1">
			<l:VerticalLayout class="sapUiContentPadding" width="100%">
				<l:content>
					<VBox>
						<Label text="{i18n>Id}" design="Bold"/>
						<Input id="idP1" value="" enabled="true" liveChange="validate" />
						<Label text="{i18n>Name}" design="Bold"/>
						<Input id="idName1" value="" enabled="true" liveChange="validate" />
						<Label text="{i18n>Dept}" design="Bold"/>
						<Input id="idDept1" value="" enabled="true" liveChange="validate" />
						<Label text="{i18n>Phone}" design="Bold"/>
						<Input id="idPhone1" value="" enabled="true" liveChange="validate" />
						<Label text="{i18n>Addr}" design="Bold"/>
						<Input id="idAddr1" value="" enabled="true" liveChange="validate" />
						<Label text="{i18n>Salary}" design="Bold"/>
						<Input id="idSalary1" value="" enabled="true" liveChange="validate" />
					</VBox>
				</l:content>
			</l:VerticalLayout>
		</f:SimpleForm>
		<buttons>
			<!--<beginButton>-->
			<Button type="Accept" text="Add" press="onSave"/>
			<!--</beginButton>-->
			<!--<endButton>-->
			<Button type="Reject"  text="{i18n>XLBL_Close}" press="onClose1"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>